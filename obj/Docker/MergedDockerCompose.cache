services:
  fahrtenbuch_backend:
    build:
      context: /Users/tabu/Projects/Backend
      dockerfile: Fahrtenbuch_Backend/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: fahrtenbuchbackend
    ports:
    - target: 80
    - target: 443
    volumes:
    - /Users/tabu/.aspnet/https:/root/.aspnet/https:ro
    - /Users/tabu/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
version: '3.4'